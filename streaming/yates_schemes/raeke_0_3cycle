s1 -> s2 :
[(s1,s2)] @ 1.000000

s1 -> s3 :
[(s1,s3)] @ 0.500000
[(s1,s2), (s2,s3)] @ 0.500000

s2 -> s1 :
[(s2,s1)] @ 1.000000

s2 -> s3 :
[(s2,s3)] @ 0.500000
[(s2,s1), (s1,s3)] @ 0.500000

s3 -> s1 :
[(s3,s1)] @ 0.500000
[(s3,s2), (s2,s1)] @ 0.500000

s3 -> s2 :
[(s3,s2)] @ 0.500000
[(s3,s1), (s1,s2)] @ 0.500000


